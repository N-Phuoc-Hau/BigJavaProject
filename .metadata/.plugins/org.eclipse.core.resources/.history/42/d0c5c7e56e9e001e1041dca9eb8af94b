package BaiTapLonOOP;

public class Main{
	public static void main(String[] agrs){
		int chon;
		do{
			chon = 0;
			switch(chon){
				case 1:{
					System.out.print("Quan ly nhan vien");
				    break;
				}
				case 2:{
					System.out.print("Quan ca phe");
		
				    break;
				}
				case 3:{
					System.out.print("Cham soc khach hang");
					break;
				}
				case 4:{
					System.out.print("Thoat");
					break;
				}
				  default:{
				}
			}
		}while(chon > 4 || chon < 1);
	}
}