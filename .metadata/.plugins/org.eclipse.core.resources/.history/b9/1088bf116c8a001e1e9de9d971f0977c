package BTH5;

import java.text.ParseException;

public class Test {

	
	public static void main(String agrs[]) throws ParseException{
		HocVien hv1 = new HocVien("Nguyen Van A", "Nam", "Vinh Long", "02/06/2004");
		HocVien hv2 = new HocVien("Nguyen Van B", "Nam", "Vinh Long", "02/06/2004");
		HocVien hv3 = new HocVien("Nguyen Van C", "Nam", "Vinh Long", "02/06/2004");

		QuanLyHocVien DS = new QuanLyHocVien();
		DS.them(hv1);
		DS.them(hv2);
		DS.them(hv3);
		DS.hienThi();
	}
}
