package test;

public class main {
	public static void main(String agrs[]){
		PhanSo A = new PhanSo(2,3);
		PhanSo B = new PhanSo(4,2);

		PhanSo[] ds ={
				new PhanSo(2,3),new PhanSo(1,4),new PhanSo(5,3),new PhanSo(3,7)
		};
		for(PhanSo ps : ds){
			ps.hienThi();
			System.out.println("");
		}
		PhanSo tong = new PhanSo(0 , 1);
		for(PhanSo ps : ds){
			tong.cong(ps);
		}
		System.out.printf("Tong cac Phan So la:%d\n",tong);
	}
}
